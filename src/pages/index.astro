import Radio from '@/components/ui/radio.astro';
import * as Select from '@/components/ui/select';
import Separator from '@/components/ui/separator.astro';
import Skeleton from '@/components/ui/skeleton.astro';
import Slider from '@/components/ui/slider.astro';
import Switch from '@/components/ui/switch.astro';
import * as Table from '@/components/ui/table';
import * as Tabs from '@/components/ui/tabs';
import Textarea from '@/components/ui/textarea.astro';
import * as Toast from '@/components/ui/toast';
import Toggle from '@/components/ui/toggle.astro';
import Tooltip from '@/components/ui/tooltip.astro';
import Layout from '@/layouts/index.astro';
import {
  AlignCenter,
  AlignLeft,
  AlignRight,
  Archive,
  ArrowDown,
  ArrowUp,
  BadgeAlert,
  Bell,
  Bold,
  Check,
  Facebook,
  Folder,
  Heart,
  Home,
  Image,
  Inbox,
  Instagram,
  Italic,
  Loader,
  LogOut,
  Mail,
  Menu as MenuIcon,
  Power,
  Send,
  Settings,
  Settings2,
  Tag,
  Trash,
  Twitter,
  Underline,
  User,
  Volume,
  Volume2,
  Youtube,
} from 'lucide-astro';
---

<Layout>
  <div class="mx-auto my-24 grid max-w-screen-xl grid-cols-[auto_1fr] items-start gap-40">
    <div class="sticky top-24 rounded border px-6 py-4">
      <h1 class="font-semibold tracking-widest">COMPONENTS</h1>
      <Separator class="mb-6 mt-2" />
      <ul class="flex flex-col gap-1">
        <Label><Checkbox checked disabled /> Accordion</Label>
