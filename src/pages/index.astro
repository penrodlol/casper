        <Label><Checkbox checked disabled /> Avatar</Label>
        <Label><Checkbox checked disabled /> Badge</Label>
        <Label><Checkbox checked disabled /> Breadcrumbs</Label>
        <Label><Checkbox checked disabled /> Button</Label>
        <Label><Checkbox checked disabled /> Carousel</Label>
        <Label><Checkbox checked disabled /> Checkbox</Label>
        <Label><Checkbox /> Chip</Label>
        <Label><Checkbox /> Code</Label>
        <Label><Checkbox checked disabled /> Collapsible</Label>
        <Label><Checkbox checked disabled /> Date Picker</Label>
        <Label><Checkbox checked disabled /> Dialog</Label>
        <Label><Checkbox checked disabled /> Drawer</Label>
        <Label><Checkbox checked disabled /> Form</Label>
        <Label><Checkbox checked disabled /> Input</Label>
        <Label><Checkbox /> Input Number</Label>
        <Label><Checkbox /> Input Password</Label>
        <Label><Checkbox /> Input Pin</Label>
        <Label><Checkbox checked disabled /> Label</Label>
        <Label><Checkbox checked disabled /> Menu</Label>
        <Label><Checkbox checked disabled /> Menubar</Label>
        <Label><Checkbox checked disabled /> Navigation Menu</Label>
        <Label><Checkbox checked disabled /> Note</Label>
        <Label><Checkbox checked disabled /> Pagination</Label>
        <Label><Checkbox checked disabled /> Popover</Label>
        <Label><Checkbox checked disabled /> Progress</Label>
        <Label><Checkbox checked disabled /> Radial Progress</Label>
        <Label><Checkbox checked disabled /> Radio Group</Label>
        <Label><Checkbox /> Rating</Label>
        <Label><Checkbox checked disabled /> Select</Label>
        <Label><Checkbox checked disabled /> Separator</Label>
        <Label><Checkbox checked disabled /> Skeleton</Label>
        <Label><Checkbox /> Shortcut</Label>
        <Label><Checkbox checked disabled /> Slider</Label>
        <Label><Checkbox checked disabled /> Switch</Label>
        <Label><Checkbox checked disabled /> Tabs</Label>
        <Label><Checkbox checked disabled /> Table</Label>
        <Label><Checkbox checked disabled /> Textarea</Label>
        <Label><Checkbox /> Theme</Label>
        <Label><Checkbox checked disabled /> Toast</Label>
        <Label><Checkbox checked disabled /> Toggle</Label>
        <Label><Checkbox checked disabled /> Tooltip</Label>
